{
  "package": {
    "name": "sandbox-test-support",
    "repo": "sandbox-maven-repository",
    "subject": "jeslopalo",
    "github_use_tag_release_notes": true
  },
  "version": {
    "name": "$TRAVIS_TAG",
    "desc": "This is a version",
    "released": "$BUILD_TIMESTAMP",
    "vcs_tag": "$TRAVIS_TAG",
    "gpgSign": false
  },
  "files": [
    {
      "includePattern": "target/(.*-[0-9]+\\.[0-9]+\\.[0-9]+\\.pom)",
      "uploadPattern": "/$1"
    },
    {
      "includePattern": "target/(.*-[0-9]+\\.[0-9]+\\.[0-9]+\\.jar)",
      "uploadPattern": "/$1"
    }
  ],
  "publish": true
}

